{
  "info": {
    "name": "Smart Parking Analytics API",
    "description": "Collection for testing all analytics APIs",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "variable": [
    {
      "key": "base_url",
      "value": "http://localhost:8000",
      "type": "string"
    }
  ],
  "item": [
    {
      "name": "1. Dashboard Summary",
      "request": {
        "method": "GET",
        "header": [],
        "url": {
          "raw": "{{base_url}}/api/analytics/dashboard/",
          "host": ["{{base_url}}"],
          "path": ["api", "analytics", "dashboard", ""]
        },
        "description": "Get overall dashboard summary statistics"
      }
    },
    {
      "name": "2. Zone Occupancy",
      "request": {
        "method": "GET",
        "header": [],
        "url": {
          "raw": "{{base_url}}/api/analytics/zones/",
          "host": ["{{base_url}}"],
          "path": ["api", "analytics", "zones", ""]
        },
        "description": "Get occupancy statistics for each zone"
      }
    },
    {
      "name": "3. Revenue Report",
      "request": {
        "method": "GET",
        "header": [],
        "url": {
          "raw": "{{base_url}}/api/analytics/revenue/?from=2024-01-01&to=2024-01-31",
          "host": ["{{base_url}}"],
          "path": ["api", "analytics", "revenue", ""],
          "query": [
            {
              "key": "from",
              "value": "2024-01-01",
              "description": "Start date (YYYY-MM-DD)"
            },
            {
              "key": "to",
              "value": "2024-01-31",
              "description": "End date (YYYY-MM-DD)"
            }
          ]
        },
        "description": "Get revenue report for specified date range"
      }
    },
    {
      "name": "4. Peak Hours",
      "request": {
        "method": "GET",
        "header": [],
        "url": {
          "raw": "{{base_url}}/api/analytics/peak-hours/",
          "host": ["{{base_url}}"],
          "path": ["api", "analytics", "peak-hours", ""]
        },
        "description": "Get peak hours analysis based on entry times"
      }
    },
    {
      "name": "5. Active Sessions",
      "request": {
        "method": "GET",
        "header": [],
        "url": {
          "raw": "{{base_url}}/api/analytics/active-sessions/",
          "host": ["{{base_url}}"],
          "path": ["api", "analytics", "active-sessions", ""]
        },
        "description": "Get all currently active parking sessions"
      }
    },
    {
      "name": "6. Completed Sessions",
      "request": {
        "method": "GET",
        "header": [],
        "url": {
          "raw": "{{base_url}}/api/analytics/completed-sessions/",
          "host": ["{{base_url}}"],
          "path": ["api", "analytics", "completed-sessions", ""]
        },
        "description": "Get recently completed parking sessions"
      }
    },
    {
      "name": "7. Vehicle History",
      "request": {
        "method": "GET",
        "header": [],
        "url": {
          "raw": "{{base_url}}/api/analytics/vehicle/DL-01-AB-1234/",
          "host": ["{{base_url}}"],
          "path": ["api", "analytics", "vehicle", "DL-01-AB-1234", ""]
        },
        "description": "Get parking history for a specific vehicle"
      }
    },
    {
      "name": "8. Payment Analytics",
      "request": {
        "method": "GET",
        "header": [],
        "url": {
          "raw": "{{base_url}}/api/analytics/payments/",
          "host": ["{{base_url}}"],
          "path": ["api", "analytics", "payments", ""]
        },
        "description": "Get payment analytics and statistics"
      }
    },
    {
      "name": "9. Slot Usage",
      "request": {
        "method": "GET",
        "header": [],
        "url": {
          "raw": "{{base_url}}/api/analytics/slots/",
          "host": ["{{base_url}}"],
          "path": ["api", "analytics", "slots", ""]
        },
        "description": "Get slot usage analytics"
      }
    }
  ]
}